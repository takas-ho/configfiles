<use_system_file_icons>applications</use_system_file_icons>
<show_hidden_files>true</show_hidden_files>
<show_ds_store_files>true</show_ds_store_files>
<auto_size_columns>true</auto_size_columns>
<show_folders_first>true</show_folders_first>
</file_table>
<find_file>
<archives>false</archives>
<ignore_hidden>true</ignore_hidden>
<case_sensitive>false</case_sensitive>
<search_hex>false</search_hex>
<encoding>UTF-8</encoding>
<subdirectories>true</subdirectories>
</find_file>
</preferences>
